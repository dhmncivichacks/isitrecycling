{
  "name": "recycle",
  "version": "1.0.0",
  "description": "Is it recycling?",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint src spec",
    "test": "jasmine",
    "watch:test": "chokidar --initial 'src/**/*.js' 'spec/*.js' -c 'npm test'",
    "build:js": "browserify src/index.js -t babelify | uglifyjs --compress --mangle > dist/build.js",
    "watch:js": "watchify src/index.js -t babelify -g livereactload --debug --verbose -o dev/build.js",
    "reload:js": "npm run watch:js & livereactload monitor dev/build.js & wait",
    "build:html": "ncp src/index.html dist/index.html",
    "watch:html": "chokidar --initial 'src/*.html' -c 'ncp src/index.html dev/index.html'",
    "reload:html": "npm run watch:html & browser-sync start --server dev --no-ui --files 'dev/index.html'",
    "predev": "rimraf dev && mkdirp dev",
    "dev": "npm run reload:js & npm run reload:html",
    "prebuild": "npm run test --silent && rimraf dist && mkdirp dist",
    "build": "NODE_ENV=production npm run build:js && npm run build:html",
    "prepublish:pages": "npm run build",
    "publish:pages": "gh-pages --dist dist"
  },
  "author": "Chris Jaure",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.1.2",
    "es6-promise": "^2.2.0",
    "jss": "^1.0.8",
    "jss-camel-case": "^0.1.1",
    "jss-px": "^0.1.2",
    "jss-vendor-prefixer": "^0.2.1",
    "material-ui": "^0.9.0",
    "moment": "^2.10.3",
    "react": "^0.13.3",
    "react-jss": "^1.0.0",
    "react-tap-event-plugin": "^0.1.7"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.15",
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.6",
    "browserify": "^8.1.3",
    "chokidar-cli": "^0.2.1",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "gh-pages": "^0.3.0",
    "jasmine-es6": "0.0.7",
    "livereactload": "^0.5.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.4.0",
    "uglify-js": "^2.4.23",
    "watchify": "^2.6.2"
  }
}
